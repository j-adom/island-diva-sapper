{
  "private": true,
  "name": "sanity-sapper-blog-web",
  "description": "Sapper frontend",
  "version": "0.0.15",
  "scripts": {
    "dev": "sapper dev",
    "build": "npm run build:tailwindcss && sapper export && mv ./__sapper__/export ./public",
    "export": "npm run build:tailwindcss && sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "watch:tailwindcss": "postcss static/assets/css/tailwind.css -o static/assets/css/main.css -w",
    "build:tailwindcss": "cross-env NODE_ENV=production postcss static/assets/css/tailwind.css -o static/assets/css/main.css",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@sanity/client": "^1.149.18",
    "@sanity/image-url": "^0.140.19",
    "compression": "^1.7.4",
    "lazysizes": "^5.2.2",
    "groq": "^0.147.8",
    "polka": "^1.0.0-next.11",
    "sirv": "^0.4.6",
    "svelte-awesome": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@movingbrands/svelte-portable-text": "0.0.7",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.3",
    "@sanity/block-content-to-hyperscript": "^2.0.10",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-nested": "^4.2.3",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.16",
    "svelte": "^3.24.0",
    "svelte-fa": "^2.1.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.0.8",
    "tailwindcss": "^1.5.2"
  }
}
